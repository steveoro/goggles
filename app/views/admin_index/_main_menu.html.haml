.navbar.navbar-fixed-top
  .navbar-inner#goggles-inner-navbar
    %a.goggles-brand(href="#{goggles_admin_index_path()}")
      %span &nbsp;
      = AGEX_APP_NAME
      %small.text-error
        %strong= t('admin_index.title')
    = render_navigation( context: 'admin_index', expand_all: true, renderer: :bootstrap )

.goggles-breadcrumb
  = render_navigation( context: 'admin_index', renderer: :breadcrumbs, prefix: "#{I18n.t(:you_are_here)}&nbsp;", join_with: " &gt; " )
