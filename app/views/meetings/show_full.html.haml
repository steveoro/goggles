- cache( cache_key_for_meeting( 'show_full', @meeting.id, @max_mir_updated_at, @team ? @team.id : 0 ) ) do
  = render( partial: 'tabs_and_radio_title' )

  .row
    %ul.offset10.span2.nav.nav-tab.nav-stacked.navbar-fixed-bottom.goggles-divlist-odd
      %li
        %a(href="#topOfPage")= I18n.t(:top_of_page)
      - @meeting_events_list.each do |me|
        %li
          %a(href="\##{me.event_type.code}")= me.event_type.i18n_description

  = render( partial: 'meeting_header' )
  .row-fluid
    .span12
      = render( partial: 'full_results_grid' )

