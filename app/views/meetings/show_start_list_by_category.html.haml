= render( partial: 'meeting_header' )
= render( partial: 'tabs_and_radio_title' )
- cache( cache_key_for_meeting( 'show_start_list_by_category', @meeting.id, @max_entry_updated_at, @team ? @team.id : 0 ) ) do
  .row
    %ul.offset10.span2.nav.nav-tab.nav-stacked.navbar-fixed-bottom.goggles-divlist-odd
      %li
        = link_to(I18n.t(:top_of_page), "#pageTop", {'data-turbolinks'=>'false'})
      - @meeting_events_list.each do |me|
        %li
          = link_to("##{me.event_type.code}", {'data-turbolinks'=>'false'}) do
            = me.event_type.i18n_description
  = render( partial: 'start_list_category_grid' )
