= render( partial: 'tabs_and_radio_title' )

.row-fluid
  .span9
    .row-fluid
      .span5
        = "#{ I18n.t('team.meeting_presence') }:"
      .span4
        = count_with_award_star( @team.meetings.collect{|row| row.id}.uniq.size )
    - @team.season_types.uniq.each do |season_type|
      .row-fluid
        .span5
          = "#{ I18n.t('team.season_presence') } #{season_type.get_full_name}"
        .span4
          = @team.seasons.where(season_type_id: season_type.id).count
.row-fluid
  %p
