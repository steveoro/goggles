%tr
  %td
    = meeting.decorate.get_linked_full_name_with_date
  %td.goggles-rightalign
    - if meeting_individual_result.reaction_time > 0
      = meeting_individual_result.reaction_time
  - passage_list.each_with_index do |passage, index|
    %td.goggles-rightalign
      - if passages
        - passage_detail = passages.where(['passage_types.length_in_meters = ?', passage]).first  
        - if passage_detail
          = passage_detail.get_timing
  %td.goggles-rightalign
    = meeting_individual_result.get_timing
  %td.goggles-rightalign
    - if meeting_individual_result.standard_points > 0
      = format_score( meeting_individual_result.standard_points )
  %td.goggles-rightalign
    = meeting_individual_result.rank
